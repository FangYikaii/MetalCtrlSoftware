<UserControl xmlns:UC="clr-namespace:MetalizationSystem.Views.UC" 
             x:Class="MetalizationSystem.Views.StationDebugging"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:locals="clr-namespace:MetalizationSystem"
             xmlns:views="clr-namespace:MetalizationSystem.Views"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" xmlns:viewmodels="clr-namespace:MetalizationSystem.ViewModels" d:DataContext="{d:DesignInstance Type=viewmodels:StationDebuggingViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <locals:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
        <!-- 全局TextBlock样式，确保整个用户控件中所有文本都没有下划线 -->
        <Style TargetType="TextBlock">
            <Setter Property="TextDecorations" Value="None"/>
        </Style>
        
        <!-- IsLowLiquidConverter转换器 - 将TankNumber转换为对应的IsLowLiquid属性值 -->
        <views:IsLowLiquidConverter x:Key="IsLowLiquidConverter"/>
    </UserControl.Resources>
    <StackPanel>
        <Expander
    HorizontalAlignment="Stretch"
    Header="配液工作站">
            <StackPanel Margin="5" Orientation="Horizontal"  HorizontalAlignment="Left">

                <GroupBox
  Width="300"
  materialDesign:ColorZoneAssist.Mode="SecondaryMid"
  Header="溶剂"
  Style="{StaticResource MaterialDesignCardGroupBox}">
                    <GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <StackPanel
        Orientation="Horizontal">
                                <materialDesign:PackIcon
          Width="32"
          Height="32"
          VerticalAlignment="Center"
          Kind="ImageArea" />
                                <TextBlock
          Margin="8,0,0,0"
          VerticalAlignment="Center"
          Style="{StaticResource MaterialDesignSubtitle1TextBlock}"
          Text="{Binding}" />
                            </StackPanel>
                        </DataTemplate>                        
                    </GroupBox.HeaderTemplate>
                    <StackPanel>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content="溶剂1用量: "/>
                            <TextBox Text="{Binding SolventMargin1}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                        <StackPanel Height="5"/>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content="溶剂2用量: "/>
                            <TextBox x:Name="SolventMargin2TextBox" Text="{Binding SolventMargin2}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                        <StackPanel Height="5"/>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content="溶剂3用量: "/>
                            <TextBox Text="{Binding SolventMargin3}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                        <StackPanel Height="5"/>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content="溶剂4用量: "/>
                            <TextBox Text="{Binding SolventMargin4}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                    </StackPanel>
                </GroupBox>
            
                <StackPanel Width="30"/>
                <GroupBox
Width="300"
materialDesign:ColorZoneAssist.Mode="SecondaryMid"
Header="过渡液"
Style="{StaticResource MaterialDesignCardGroupBox}">
                    <GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <StackPanel
      Orientation="Horizontal">
                                <materialDesign:PackIcon
        Width="32"
        Height="32"
        VerticalAlignment="Center"
        Kind="ImageArea" />
                                <TextBlock
        Margin="8,0,0,0"
        VerticalAlignment="Center"
        Style="{StaticResource MaterialDesignSubtitle1TextBlock}"
        Text="{Binding}" />
                            </StackPanel>
                        </DataTemplate>
                    </GroupBox.HeaderTemplate>
                    <StackPanel>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content=" 过渡液1用量: "/>
                            <TextBox Text="{Binding TransitionFluidMargin1}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                        <StackPanel Height="5"/>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content=" 过渡液2用量: "/>
                            <TextBox Text="{Binding TransitionFluidMargin2}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                        <StackPanel Height="5"/>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content=" 过渡液3用量: "/>
                            <TextBox Text="{Binding TransitionFluidMargin3}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                        <StackPanel Height="5"/>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content=" 过渡液4用量: "/>
                            <TextBox Text="{Binding TransitionFluidMargin4}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                        <StackPanel Height="5"/>
                        <StackPanel Orientation="Horizontal" >
                            <Label Content=" 过渡液5用量: "/>
                            <TextBox Text="{Binding TransitionFluidMargin5}" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="10" Foreground="Black"/>
                            <Label Content=" ml"/>
                        </StackPanel>
                    </StackPanel>
                </GroupBox>
                
                <StackPanel Width="30"/>
                <StackPanel>
                    <StackPanel Height="30"/>
                    <locals:ButtonEx Margin="5"  Command="{Binding FixPumpTestCommand}"   Grid.Column="2" Content="测试" FontFamily="kaiti" FontSize="10" Foreground="#1afa29" OnMouseOverForegroundColor="#1afa29" ImageSize="40"
PathData="M704 128a32 32 0 0 1 0 64h-21.333333v160.789333a10.666667 10.666667 0 0 0 1.088 4.693334l180.874666 369.6c28.48 58.218667 4.384 128.490667-53.813333 156.970666A117.333333 117.333333 0 0 1 759.242667 896H264.746667c-64.8 0-117.333333-52.533333-117.333334-117.333333a117.333333 117.333333 0 0 1 11.946667-51.573334l180.874667-369.621333a10.666667 10.666667 0 0 0 1.088-4.693333V192h-21.333334a32 32 0 0 1-31.946666-30.122667L288 160a32 32 0 0 1 32-32z m51.968 522.666667H268.010667l-51.157334 104.554666A53.333333 53.333333 0 0 0 264.757333 832H759.253333a53.333333 53.333333 0 0 0 47.893334-76.778667L755.978667 650.666667zM618.666667 192H405.333333v160.789333a74.666667 74.666667 0 0 1-7.594666 32.821334L299.328 586.666667h425.322667l-98.389334-201.066667A74.666667 74.666667 0 0 1 618.666667 352.8V192z">
                    </locals:ButtonEx>
                </StackPanel>
            </StackPanel>
        </Expander>
      
        <Separator Margin="3"/>
        <Expander
    Header="机械手/第七轴">
            <StackPanel Margin="5" Orientation="Horizontal"  HorizontalAlignment="Left">
                <GroupBox
  Width="300"
  materialDesign:ColorZoneAssist.Mode="SecondaryMid"
  Header="轴控制"
  Style="{StaticResource MaterialDesignCardGroupBox}">
                    <GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <StackPanel
        Orientation="Horizontal">
                                <materialDesign:PackIcon
          Width="32"
          Height="32"
          VerticalAlignment="Center"
          Kind="ImageArea" />
                                <TextBlock
          Margin="8,0,0,0"
          VerticalAlignment="Center"
          Style="{StaticResource MaterialDesignSubtitle1TextBlock}"
          Text="{Binding}" />
                            </StackPanel>
                        </DataTemplate>
                    </GroupBox.HeaderTemplate>
                    <UC:Axis7Uc/>
                </GroupBox>
                <StackPanel Width="30"/>
                <GroupBox
materialDesign:ColorZoneAssist.Mode="SecondaryMid"
Header="机器人"
Style="{StaticResource MaterialDesignCardGroupBox}">
                    <GroupBox.HeaderTemplate>
                        <DataTemplate>
                            <StackPanel
      Orientation="Horizontal">
                                <materialDesign:PackIcon
        Width="32"
        Height="32"
        VerticalAlignment="Center"
        Kind="ImageArea" />
                                <TextBlock
        Margin="8,0,0,0"
        VerticalAlignment="Center"
        Style="{StaticResource MaterialDesignSubtitle1TextBlock}"
        Text="{Binding}" />
                            </StackPanel>
                        </DataTemplate>
                    </GroupBox.HeaderTemplate>
                    <UC:RobotUc  PlateNum="50"/>
                </GroupBox>
                <StackPanel Width="30"/>
            </StackPanel>
        </Expander>


        
        <Separator Margin="3"/>
        <Expander
    HorizontalAlignment="Stretch"
    Header="过渡槽">
            <Grid Margin="2">
                <DataGrid 
                    ItemsSource="{Binding ReactionTankInfos}"
                    AutoGenerateColumns="False"
                    CanUserAddRows="False"
                    CanUserDeleteRows="False"
                    CanUserResizeRows="False"
                    RowHeaderWidth="0"
                    RowHeight="35"
                    VerticalContentAlignment="Center"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Style="{StaticResource MaterialDesignDataGrid}"
                    ColumnHeaderStyle="{StaticResource MaterialDesignDataGridColumnHeader}"
                    CellStyle="{StaticResource MaterialDesignDataGridCell}"
                    Background="White" >
                        <!-- 全局样式，确保覆盖所有可能的TextDecorations设置 -->
                        <DataGrid.Resources>
                            <!-- 移除所有TextBlock的下划线 -->
                            <Style TargetType="TextBlock">
                                <Setter Property="TextDecorations" Value="None"/>
                            </Style>
                            
                            <!-- 简化DataGridCell样式，确保文本居中对齐 -->
                            <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                <Setter Property="VerticalAlignment" Value="Center"/>
                                <Setter Property="HorizontalAlignment" Value="Center"/>
                            </Style>
                        </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="序号" Binding="{Binding Id}" Width="60" IsReadOnly="True"/>
                        <DataGridTextColumn Header="槽位" Binding="{Binding TankNumber}" Width="Auto" IsReadOnly="True">
                            <DataGridTextColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="HorizontalAlignment" Value="Left"/>
                                </Style>
                            </DataGridTextColumn.CellStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="名称" Binding="{Binding Name}" Width="Auto" IsReadOnly="True">
                            <DataGridTextColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="HorizontalAlignment" Value="Left"/>
                                </Style>
                            </DataGridTextColumn.CellStyle>
                        </DataGridTextColumn>
                        <DataGridCheckBoxColumn Header="搅拌" Binding="{Binding IsMixing, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="Auto">
                            <DataGridCheckBoxColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="IsEnabled" Value="{Binding IsMixingEnabled}"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                </Style>
                            </DataGridCheckBoxColumn.CellStyle>
                        </DataGridCheckBoxColumn>
                        <!-- 液位列 - 指示灯，液位低时显示红色 -->
                        <DataGridTemplateColumn Header="液位" Width="Auto">
                            <DataGridTemplateColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}"/>
                            </DataGridTemplateColumn.CellStyle>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <Border Width="13" Height="13" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <Border.Style>
                                            <Style TargetType="Border">
                                                <!-- 默认样式 - 与背景色相同 -->
                                                <Setter Property="Background" Value="{DynamicResource MaterialDesignPaper}"/>
                                                <Setter Property="BorderBrush" Value="{DynamicResource MaterialDesignDivider}"/>
                                                <Setter Property="BorderThickness" Value="1"/>
                                                <Setter Property="CornerRadius" Value="0"/>
                                                    
                                                <!-- 样式触发器 -->
                                                <Style.Triggers>
                                                    <!-- 为Tank30设置特殊样式 - 不显示液位功能 -->
                                                    <DataTrigger Binding="{Binding TankNumber}" Value="Tank30">
                                                        <Setter Property="Visibility" Value="Hidden"/>
                                                    </DataTrigger>
                                                        
                                                    <!-- 其他Tank，当IsLowLiquid为true时显示红色 -->
                                                    <DataTrigger Binding="{Binding Path=TankNumber, Converter={StaticResource IsLowLiquidConverter}}" Value="true">
                                                        <Setter Property="Background" Value="Red"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Border.Style>
                                    </Border>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridCheckBoxColumn Header="进液使能" Binding="{Binding IsChargingEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="Auto">
                            <DataGridCheckBoxColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="IsEnabled" Value="{Binding IsChargingControlEnabled}"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                </Style>
                            </DataGridCheckBoxColumn.CellStyle>
                        </DataGridCheckBoxColumn>
                        <DataGridTemplateColumn Header="进液" Width="Auto">
                            <DataGridTemplateColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}"/>
                            </DataGridTemplateColumn.CellStyle>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <Border Width="13" Height="13" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <Border.Style>
                                            <Style TargetType="Border">
                                                <!-- 默认样式 - 与背景色相同 -->
                                                <Setter Property="Background" Value="{DynamicResource MaterialDesignPaper}"/>
                                                <Setter Property="BorderBrush" Value="{DynamicResource MaterialDesignDivider}"/>
                                                <Setter Property="BorderThickness" Value="1"/>
                                                <Setter Property="CornerRadius" Value="0"/>
                                                     
                                                <!-- 样式触发器 -->
                                                <Style.Triggers>
                                                    <!-- 为Tank30设置特殊样式 - 不显示进液功能 -->
                                                    <DataTrigger Binding="{Binding TankNumber}" Value="Tank30">
                                                        <Setter Property="Visibility" Value="Hidden"/>
                                                    </DataTrigger>
                                                         
                                                    <!-- 其他Tank，当IsChargingFluid为true时显示绿色 -->
                                                    <DataTrigger Binding="{Binding Path=IsChargingFluid}" Value="true">
                                                        <Setter Property="Background" Value="Green"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Border.Style>
                                    </Border>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Header="进液超时" Width="Auto">
                            <DataGridTemplateColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}"/>
                            </DataGridTemplateColumn.CellStyle>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <Border Width="13" Height="13" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <Border.Style>
                                            <Style TargetType="Border">
                                                <!-- 默认样式 - 与背景色相同 -->
                                                <Setter Property="Background" Value="{DynamicResource MaterialDesignPaper}"/>
                                                <Setter Property="BorderBrush" Value="{DynamicResource MaterialDesignDivider}"/>
                                                <Setter Property="BorderThickness" Value="1"/>
                                                <Setter Property="CornerRadius" Value="0"/>
                                                     
                                                <!-- 样式触发器 -->
                                                <Style.Triggers>
                                                    <!-- 为Tank30设置特殊样式 - 不显示进液超时功能 -->
                                                    <DataTrigger Binding="{Binding TankNumber}" Value="Tank30">
                                                        <Setter Property="Visibility" Value="Hidden"/>
                                                    </DataTrigger>
                                                         
                                                    <!-- 其他Tank，当IsChargeTimeout为true时显示红色 -->
                                                    <DataTrigger Binding="{Binding Path=IsChargeTimeout}" Value="true">
                                                        <Setter Property="Background" Value="Red"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Border.Style>
                                    </Border>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridCheckBoxColumn Header="排液" Binding="{Binding IsDrainingEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="Auto">
                            <DataGridCheckBoxColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="IsEnabled" Value="{Binding IsDrainingControlEnabled}"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                </Style>
                            </DataGridCheckBoxColumn.CellStyle>
                        </DataGridCheckBoxColumn>
                        <DataGridTextColumn Header="目标温度(℃)" Binding="{Binding TargetTemperature, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, StringFormat={}{0:F1}}" Width="Auto">
                            <DataGridTextColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank20">
                                            <Setter Property="IsEnabled" Value="False"/>
                                            <Setter Property="Visibility" Value="Hidden"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank21">
                                            <Setter Property="IsEnabled" Value="False"/>
                                            <Setter Property="Visibility" Value="Hidden"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank22">
                                            <Setter Property="IsEnabled" Value="False"/>
                                            <Setter Property="Visibility" Value="Hidden"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGridTextColumn.CellStyle>
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="TextAlignment" Value="Center"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                            <DataGridTextColumn.EditingElementStyle>
                                <Style TargetType="TextBox">
                                    <Setter Property="TextAlignment" Value="Center"/>
                                    <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                </Style>
                            </DataGridTextColumn.EditingElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="实际温度(℃)" Binding="{Binding ActualTemperature}" Width="Auto" IsReadOnly="True">
                            <DataGridTextColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank20">
                                            <Setter Property="Visibility" Value="Hidden"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank21">
                                            <Setter Property="Visibility" Value="Hidden"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank22">
                                            <Setter Property="Visibility" Value="Hidden"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGridTextColumn.CellStyle>
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="TextAlignment" Value="Center"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridCheckBoxColumn Header="加热" Binding="{Binding IsHeatingEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="Auto">
                            <DataGridCheckBoxColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank20">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank21">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank22">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGridCheckBoxColumn.CellStyle>
                        </DataGridCheckBoxColumn>
                        <DataGridCheckBoxColumn Header="鼓泡" Binding="{Binding IsBubblingEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="Auto">
                            <DataGridCheckBoxColumn.CellStyle>
                                <Style TargetType="DataGridCell" BasedOn="{StaticResource MaterialDesignDataGridCell}">
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank23">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank40">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank41">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank42">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank43">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank50">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank51">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank53">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding TankNumber}" Value="Tank30">
                                            <Setter Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGridCheckBoxColumn.CellStyle>
                        </DataGridCheckBoxColumn>
                    </DataGrid.Columns>
                </DataGrid>
            </Grid>
        </Expander>
        
        <Separator Margin="3"/>
        <Expander
    HorizontalAlignment="Stretch"
    Header="粘结力测试">
            <!-- 粘结力测试内容将在此处添加 -->
        </Expander>

    </StackPanel>
</UserControl>
